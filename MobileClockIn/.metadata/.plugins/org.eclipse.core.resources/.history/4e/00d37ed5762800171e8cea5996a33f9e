package com.mycuteblog.service.impl;

import com.mycuteblog.dao.BaseDao;
import com.mycuteblog.model.ApiRequest;
import com.mycuteblog.service.BaseService;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;
import org.springframework.transaction.annotation.Transactional;

@Service
@Transactional
public class BaseServiceImpl implements BaseService {

    @Autowired
    private BaseDao baseDao;

    public void create(ApiRequest apiRequest) {
        baseDao.create(apiRequest);
    }
}
